{
    "elements": [
        {
            "tagName": "div",
            "attributes": {
                "class": "player"
            },
            "elements": [
                {
                    "tagName": "div",
                    "attributes": {
                        "class": "circleBase",
                        "id": "none"
                    },
                    "elements": [
                        {
                            "tagName": "input",
                            "attributes": {
                                "maxlength": 20,
                                "value": "name",
                                "onchange": "testForSpecialName(this)"
                            }
                        },
                        {
                            "tagName": "div",
                            "attributes": {
                                "id": "powerLevel"
                            },
                            "elements": [
                                {
                                    "tagName": "img",
                                    "attributes": {
                                        "src": "media/left_arrow.svg",
                                        "alt": "<-",
                                        "onclick": "changePower(this, -1);",
                                        "class": "clickElement"
                                    }
                                },
                                {
                                    "tagName": "input",
                                    "attributes": {
                                        "maxlength": 20,
                                        "value": 1
                                    }
                                },
                                {
                                    "tagName": "img",
                                    "attributes": {
                                        "src": "media/right_arrow.svg",
                                        "alt": "->",
                                        "onclick": "changePower(this, 1);",
                                        "class": "clickElement"
                                    }
                                }
                            ]
                        },
                        {
                            "tagName": "input",
                            "attributes": {
                                "maxlength": 20,
                                "value": "gender",
                                "onchange": "testForSignal(this);"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}